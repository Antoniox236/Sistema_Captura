<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="name" content="">
  <title>Sistema Captura</title>

  <!-- Custom fonts for this template-->
  <link href="assets/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link
    href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
    rel="stylesheet">
  <!-- Custom styles for this template-->
  <link href="assets/css/sb-admin-2.min.css" rel="stylesheet">
  <link href="assets/css/style.css" rel="stylesheet">

</head>

<body id="page-top">


  <!-- Page Wrapper -->
  <div id="wrapper">

    {{> navbar}}

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">

        {{> head}}

        <!-- Begin Page Content -->
        <div class="container-fluid">
          <!-- Page Heading -->
          <div class="row justify-content-center">
            <h1 class="h3 mb-4 text-gray-800">REGISTRO DE PADRES</h1>
          </div>
        </div>

        <!-- End of Main Content -->
        <div class="row">
          <div class="col-md-2"></div>
          <div class="col-md-8 justify-content-center">
            <!-- Outer Row -->
            <!-- Nested Row within Card Body -->
            <form>
              <div class="form-group">
                <label for="inputAddress">NOMBRE</label>
                <input type="text" class="form-control" id="inputAddress" placeholder="NOMBRE ...">
              </div>
              <div class="form-group">
                <label for="inputAddress2">APELLIDO MATERNO</label>
                <input type="text" class="form-control" id="inputAddress2" placeholder="APELLIDO MATERNO ...">
              </div>
              <div class="form-group">
                <label for="inputAddress2">APELLIDO PATERNO</label>
                <input type="text" class="form-control" id="inputAddress2" placeholder="APELLIDO PATERNO ...">
              </div>
 
              <div class="form-group">
                <label for="inputState">PADRE DEL ALUMNO</label>
                <div class="custom-file">
                  <input type="file" class="custom-file-input" id="customFile">
                  <label class="custom-file-label" for="customFile">Choose file</label>
                </div>
              </div>
              <hr>

              <button type="submit" class="btn btn-primary btn-user btn-block">Sign in</button>
            </form>
            <hr>
          </div>
          <!-- End of Content Wrapper -->
        </div>

        <!-- Footer -->
        {{> footer}}
        <!-- End of Footer -->

      </div>
      <!-- End of Page Wrapper -->


</body>

</html>